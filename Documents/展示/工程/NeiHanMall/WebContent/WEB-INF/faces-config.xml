<?xml version="1.0" encoding="UTF-8"?>

<faces-config
    xmlns="http://java.sun.com/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd"
    version="2.0">
	<managed-bean>
		<managed-bean-name>registerBean</managed-bean-name>
		<managed-bean-class>com.neihan.backbeans.RegisterBean</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
		<managed-property>
			<property-name>email</property-name>
			<property-class>java.lang.String</property-class>
			<value></value>
		</managed-property>
		<managed-property>
			<property-name>password</property-name>
			<property-class>java.lang.String</property-class>
			<value></value>
		</managed-property>
		<managed-property>
			<property-name>password_ensure</property-name>
			<property-class>java.lang.String</property-class>
			<value></value>
		</managed-property>
		<managed-property>
			<property-name>telephone</property-name>
			<property-class>java.lang.String</property-class>
			<value></value>
		</managed-property>
		<managed-property>
			<property-name>userName</property-name>
			<property-class>java.lang.String</property-class>
			<value></value>
		</managed-property>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>loginBean</managed-bean-name>
		<managed-bean-class>com.neihan.backbeans.LoginBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>userBean</managed-bean-name>
		<managed-bean-class>com.neihan.backbeans.UserBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>editInfoBean</managed-bean-name>
		<managed-bean-class>com.neihan.backbeans.EditInfoBean</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>goodsBean</managed-bean-name>
		<managed-bean-class>com.neihan.backbeans.GoodsBean</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>cartBean</managed-bean-name>
		<managed-bean-class>com.neihan.backbeans.CartBean</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<navigation-rule>
		<display-name>register</display-name>
		<from-view-id>/register.jsp</from-view-id>
		<navigation-case>
			<from-outcome>success</from-outcome>
			<to-view-id>/login.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>homepage</display-name>
		<from-view-id>/homepage.jsp</from-view-id>
		<navigation-case>
			<from-outcome>login</from-outcome>
			<to-view-id>/login.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>homepage</display-name>
		<from-view-id>/homepage.jsp</from-view-id>
		<navigation-case>
			<from-outcome>register</from-outcome>
			<to-view-id>/register.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>login</display-name>
		<from-view-id>/login.jsp</from-view-id>
		<navigation-case>
			<from-outcome>success</from-outcome>
			<to-view-id>/homepage.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>homepage</display-name>
		<from-view-id>/homepage.jsp</from-view-id>
		<navigation-case>
			<from-outcome>myinfo</from-outcome>
			<to-view-id>/myInfo.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>cart</display-name>
		<from-view-id>/cart.jsp</from-view-id>
		<navigation-case>
			<from-outcome>homepage</from-outcome>
			<to-view-id>/homepage.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>goodsinfo</display-name>
		<from-view-id>/goodsinfo.jsp</from-view-id>
		<navigation-case>
			<from-outcome>addtoCart</from-outcome>
			<to-view-id>/docart.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>docart</display-name>
		<from-view-id>/docart.jsp</from-view-id>
		<navigation-case>
			<from-outcome>cart</from-outcome>
			<to-view-id>/cart.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>

</faces-config>
